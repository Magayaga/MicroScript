var add = |Float64: a, Float64: b| => Float64 {a + b};
var calculateArea = |Float64: radius| => Float64 {3.14159 * radius * radius};

function main() {
    console.write(add(9, 5));
    console.write("Area of circle with radius 5 = {}", calculateArea(5));
}

main();
