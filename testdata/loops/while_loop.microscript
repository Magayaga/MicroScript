function main() {
    console.write("Starting while loop test with <= operator");
    var i: Float64 = 1;
    while (i <= 5) {
        console.write("i = {}", i);
        i = i + 1;
    }
    console.write("Loop finished, final i = {}", i);
    
    // Test other comparison operators
    console.write("\nTesting other comparison operators:");
    
    var j: Int32 = 5;
    while (j >= 1) {
        console.write("j = {}", j);
        j = j - 1;
    }
    
    var k: Int32 = 1;
    while (k < 5) {
        console.write("k = {}", k);
        k = k + 1;
    }
    
    var m: Int32 = 5;
    while (m > 1) {
        console.write("m = {}", m);
        m = m - 1;
    }
    
    console.write("All tests completed.");
}

main(); 