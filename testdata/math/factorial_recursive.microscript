// Copyright (c) 2025 Cyril John Magayaga

function factorial(n: Float64) -> Float64 {
    if (n == 0) {
        return 1;
    }
    else if (n < 0) {
        return "Factorial is not defined for negative numbers";
    }
    else {
        return n * factorial(n - 1);
    }
}

function main() {
    console.write(factorial(6));
    // Output: 720.0
}

main();