// Compute the pythagorean theorem using MicroScript
// Copyright (c) 2025 Cyril John Magayaga

function pythrem(a: Float64, b: Float64) -> Float64 {
    return (a * a) + (b * b)
}

// Main function
function main() {
    var number: Float64 = pythrem(4, 3);
    console.write(number);
    console.write(pythrem(11, 8));
}

// Call the main function
main();