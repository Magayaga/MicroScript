function greet(name: String) -> String {
    return "Hello, {name}!";
}

function processUserInput(callback: (name: String) => String) -> Void {
    var name: String = "Pablo"
    console.log(callback(name));
}

function main() {
    processUserInput(greet);
    // Output: Hello, Pablo!
}

main();