import math

function main() {
    console.write(math::PI);
    console.write(math::E);
    console.write(math::TAU);
    console.write(math::PHI);
    console.write(math::sqrt(4));
    console.write(math::square(4));
    console.write(math::cube(4));
    console.write(math::cbrt(27));
    console.write(math::abs(-9));
    console.write(math::log(100));
    console.write(math::log2(100));
    console.write(math::log10(100));
}

main();