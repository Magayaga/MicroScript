    bool conditionD = not conditionC
    bool conditionE = not true
    bool conditionF = not false
    bool conditionG = !true
    bool conditionH = !false
    bool conditionI = !conditionG
    bool conditionJ = !conditionH
    console.write(conditionA);
    console.write(conditionB);
    console.write(conditionC);
    console.write(conditionD);
    console.write(conditionE);
    console.write(conditionF);
    console.write(conditionG);
    console.write(conditionH);
}

main();
